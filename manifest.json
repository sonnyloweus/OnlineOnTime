{
  "name": "OnlineOnTime",
  "version": "0.2.0",
  "description": "Never Miss an Online Class Period Again",
  "permissions": ["declarativeContent", "storage", "alarms", "notifications"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'",
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/OnlineOnTimeIcon16.png",
      "32": "images/OnlineOnTimeIcon32.png",
      "48": "images/OnlineOnTimeIcon48.png",
      "128": "images/OnlineOnTimeIcon128.png"
    }
  },
  "icons": {
    "16": "images/OnlineOnTimeIcon16.png",
    "32": "images/OnlineOnTimeIcon32.png",
    "48": "images/OnlineOnTimeIcon48.png",
    "128": "images/OnlineOnTimeIcon128.png"
  },
  "manifest_version": 2
}